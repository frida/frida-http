{
  "name": "frida-http",
  "version": "2.0.4",
  "description": "Node.js compatible “http” module for Frida",
  "keywords": [
    "frida"
  ],
  "homepage": "http://www.frida.re",
  "main": "index.js",
  "files": [
    "/index.js",
    "/lib/**/*.js"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "loose": true
              }
            ]
          ],
          "plugins": [
            [
              "@babel/plugin-transform-runtime",
              {
                "corejs": 2
              }
            ]
          ]
        }
      ]
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/frida/frida-http.git"
  },
  "bugs": {
    "url": "http://github.com/frida/frida-http/issues"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs2": "^7.1.2",
    "babelify": "^10.0.0",
    "http-parser-js": "^0.4.4"
  }
}
